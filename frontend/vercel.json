{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "installCommand": "npm install",
  "routes": [
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/LogoW.png",
      "dest": "/LogoW.png"
    },
    {
      "src": "/test-favicon.html",
      "dest": "/test-favicon.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_API_URL": "https://api.workflowguard.pro/api"
  }
} 